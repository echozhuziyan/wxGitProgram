.container{
   padding:0px;
   display: block;
}
.imgContainer{
    margin-top:2px;
    margin-bottom:36px;
    column-count: 2;
    column-gap: 2px;
    column-rule:1px solid #eeeeee;
}
.imgContainer image{
    padding-bottom: 2px;
    height: 135px;
    background-color: #eeeeee;
    text-align: center;
    width:100%;
}
.uploadButton{
    position:fixed;
    bottom:0px;
    left:0;
    right:0;
    z-index: 100;
}
.uploadButton button{
    width:100%;
    margin:0;
    height: 36px;
    line-height: 36px;
    border-radius: 0;
}
.swiper-container {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000;
    z-index: 200;
}
.swiper-container image {
    width: 100%;
    height: 100%;
}
image.deleteImg{
    float:right;
    width:30px;
    height:30px;
    position:fixed;
    bottom:10px;
    right:10px;
}
